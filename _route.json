{
    "version": 1,
    "include": ["/*"],
    "routes": [
      {
        "pattern": "/css/*",
        "headers": {
          "Content-Type": "text/css"
        },
        "static": true
      },
      {
        "pattern": "/rust-md-checker",
        "worker": {
          "name": "rust-md-checker",
          "binding": "RUST_MD_CHECKER"
        }
      },
      {
        "pattern": "/rust-md-checker/*",
        "worker": {
          "name": "rust-md-checker",
          "binding": "RUST_MD_CHECKER"
        }
      }
    ]
  }